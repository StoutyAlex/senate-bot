    "@types/node": "^16.4.10",
    "@types/properties-reader": "^2.1.1",
    "faker": "^5.5.3",
    "jest": "^27.3.1",
    "nodemon": "^2.0.15",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.3.5"
