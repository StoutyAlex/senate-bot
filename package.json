    "lodash": "^4.17.21",
    "minecraft-server-util": "^5.2.8",
    "properties-reader": "^2.2.0",
    "webhook-discord": "^3.7.8",
    "youtubei": "^0.0.1-rc.28"
  },
  "devDependencies": {
    "@aws-cdk/aws-dynamodb": "^1.139.0",
    "@aws-cdk/aws-ec2": "^1.139.0",
    "@aws-cdk/aws-ecr": "^1.139.0",
